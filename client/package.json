{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbopack",
    "build": "next build --no-lint",
    "build:fast": "next build --no-lint --no-typescript",
    "build:no-check": "next build --no-lint --no-typescript",
    "start": "next start -p 3001",
    "start:fast": "next start -p 3001 --turbopack",
    "lint": "next lint",
    "clean": "rimraf .next .turbo node_modules/.cache",
    "clean:all": "rimraf .next .turbo node_modules/.cache dist out",
    "cache:clear": "npm run clean && npm run dev",
    "cache:clear:all": "npm run clean:all && npm install && npm run dev",
    "dev:no-maps": "DISABLE_SOURCE_MAPS=true npm run dev",
    "dev:fast:no-maps": "DISABLE_SOURCE_MAPS=true npm run dev:fast",
    "analyze": "ANALYZE=true npm run build",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@types/file-saver": "^2.0.7",
    "@types/react-select": "^5.0.0",
    "clsx": "^2.1.1",
    "file-saver": "^2.0.5",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.8",
    "heroicons": "^2.2.0",
    "is-number": "^7.0.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.535.0",
    "next": "^15.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-select": "^5.10.2",
    "react-sweetalert2": "^0.6.0",
    "recharts": "^3.1.0",
    "sweetalert2": "^11.22.3",
    "world-countries": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19",
    "@types/react-select": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  }
}
